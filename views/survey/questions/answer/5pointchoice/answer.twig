{#
/**
 * 5-Point Choice - DSFR Theme
 * Inline radio buttons (1 2 3 4 5) with DSFR fr-radio-group
 */
#}

<!-- 5-Point Choice DSFR -->
<fieldset class="fr-fieldset {{coreClass}}" aria-labelledby="ls-question-text-{{name}}">
    <div class="fr-fieldset__content">
        <div class="fr-grid-row fr-grid-row--gutters">
            {% for aRow in aRows %}
                {% include './survey/questions/answer/5pointchoice/rows/item_row.twig' with aRow %}
            {% endfor %}
        </div>
    </div>
</fieldset>

<input type="hidden" name="java{{name}}" id="java{{name}}" disabled value="{{sessionValue}}" />
