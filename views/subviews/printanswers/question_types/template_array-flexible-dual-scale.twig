<div class="fr-grid-row question-container-printanswers" style="page-break-inside:avoid;">
    {% include './subviews/printanswers/partials/_question_header.twig' %}
    <div class="fr-col-lg-8">
       <table class="table table-condensed table-striped">
       {% for subquestionId, subquestion in question.subquestions %}
           {% if subquestion.question_order == 1 %}
               <tr>
                   <th></th>
                   {% for colname, value in question.dualscale_header %}
                       <th><b>{{value}}</b></th>
                   {% endfor %}
               </tr>
           {% endif %}

           <tr>
               <td>
                   {{subquestion.question}} ({{subquestion.title}})
               </td>
               {% for colname, value in subquestion.answervalues %}
                   <td>
                       {{value}}
                   </td>
               {% endfor %}
           </tr>
       {% endfor %}
       </table>
   </div>
</div>
